            "url": "modules/seo"
        },
        {
            "type": "path",
            "url": "modules/adminlte"
        },
        {
            "type": "path",
            "url": "modules/imshop"
        },
        {
            "type": "path",
            "url": "modules/webpack"
        },
        {
            "type": "path",
